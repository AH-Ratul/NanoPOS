{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "generated/prisma-client/*.js", "outDir": "dist" },
      { "include": "generated/prisma-client/*.wasm", "outDir": "dist" },
      { "include": "generated/prisma-client/*.node", "outDir": "dist" }
    ]
  }
}
